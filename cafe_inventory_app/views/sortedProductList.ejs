<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <%- include("partials/errors.ejs") %>
    <%- include('partials/navbar.ejs', {links: links}) %>
    <h2><%= header %></h2>
    <div class="sorter-selection">
        <%- include('partials/sorter.ejs', {sorter: sorter, selectedSorter: selectedSorter, searcher: searcher, searchPattern: searchPattern}) %>
    </div>
    <div class="product-list-space">
      <% searchResult.map((product) =>{ %>
        <%- include('partials/productEntry.ejs', {product: product}) %>
      <% }); %>
    </div>
  </body>
</html>
