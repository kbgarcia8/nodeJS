<nav class="menu-filter">
    <ul class="menu-filter-list">
      <li class="menu-filter-entry">
          <a class="menu-filter-entry-link" href="/menu">
            <span> All </span>
          </a>
        </li>
      <% for (let i = 0; i < filter.length; i++) { %>
        <li class="menu-filter-entry" data-text="<%= filter[i].text %>">
          <a class="menu-filter-entry-link" href="/menu/<%= filter[i].href %>">
            <span> <%= filter[i].text %> </span>
          </a>
        </li>
      <% } %>
    </ul>
  </nav>
  